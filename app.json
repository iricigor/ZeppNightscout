{
  "configVersion": "v2",
  "app": {
    "appId": "com.nightscout.zepp",
    "appName": "Nightscout",
    "appType": "app",
    "version": {
      "code": 1,
      "name": "0.1.0"
    },
    "icon": "icon.png",
    "vendor": "Nightscout",
    "description": "Connect to Nightscout and display CGM data"
  },
  "permissions": [
    "internet",
    "data:user.info"
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "1.0.0",
      "target": "1.0.1",
      "minVersion": "1.0.0"
    }
  },
  "targets": {
    "balance": {
      "module": {
        "page": {
          "pages": [
            "page/index"
          ]
        },
        "app-side": {
          "path": "app-side/index"
        }
      }
    }
  },
  "i18n": {
    "en-US": {
      "appName": "Nightscout"
    }
  }
}
